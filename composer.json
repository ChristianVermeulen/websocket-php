{
    "name": "phrity/websocket",
    "description": "WebSocket client and server",
    "license": "ISC",
    "type": "library",
    "authors": [
        {
            "name": "Fredrik Liljegren"
        },
        {
            "name": "SÃ¶ren Jensen"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phrity\\WebSocket\\": "src/",
            "WebSocket\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebSocket\\": "tests/mock"
        }
    },
    "require": {
        "php": "^8.0",
        "phrity/net-uri": "^1.2",
        "phrity/net-stream": "dev-extending as 1.1.1",
        "phrity/util-errorhandler": "^1.0",
        "psr/log": "^1.0 | ^2.0 | ^3.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "php-coveralls/php-coveralls": "^2.0",
        "phrity/net-mock": "dev-main",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../phrity-net-mock",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../phrity-net-stream",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../phrity-container",
            "options": {
                "symlink": true
            }
        }
    ]
}
